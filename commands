[ ! -d 'expressBookReviews' ] && git clone https://github.com/yk942003/expressBookReviews.git
curl -X POST "http://localhost:5000/register?username=yk&password=123"
curl -c cookies.txt -X POST "http://localhost:5000/customer/login?username=yk&password=123"
curl -b cookies.txt -X PUT "http://localhost:5000/customer/auth/review/1?review=Great"

curl -X POST "http://localhost:5000/register?username=yk2&password=123"
curl -c cookies.txt -X POST "http://localhost:5000/customer/login?username=yk2&password=123"
curl -b cookies.txt -X PUT "http://localhost:5000/customer/auth/review/1?review=Best%20book%20ever"
